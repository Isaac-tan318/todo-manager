{
  "name": "todo-manager",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-frontend": "npx playwright test",
    "test:coverage": "jest --verbose --coverage",
    "test-coverage:frontend": "node generate-coverage.js",
    "test:a11y": "npx playwright test e2e/advanced-testing.spec.ts --grep \"Accessibility\"",
    "test:visual": "npx playwright test e2e/advanced-testing.spec.ts --grep \"Visual Regression\"",
    "test:schema": "npx playwright test e2e/advanced-testing.spec.ts --grep \"API Schema\"",
    "test:performance": "npx playwright test e2e/advanced-testing.spec.ts --grep \"Performance\"",
    "test:advanced": "npx playwright test e2e/advanced-testing.spec.ts",
    "test:update-snapshots": "npx playwright test e2e/advanced-testing.spec.ts --update-snapshots",
    "test:report": "npx playwright show-report"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^5.1.0",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "zod": "^4.3.5"
  }
}
